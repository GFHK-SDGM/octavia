{
	"name": "@ltgc/octavia",
	"version": "0.0.1",
	"exports": {
		"./basic": "./basic.mjs",
		"./bridge": "./bridge.mjs",
		"./cambiare": "./cambiare.mjs",
		"./disp": "./disp.mjs",
		"./state": "./state.mjs",
		"./xp_basic": "./xp_basic.mjs",
		"./xp_state": "./xp_state.mjs"
	},
	"imports": {
		"@octavia": "../dist",
		"@octavia/basic": "../dist/basic.mjs",
		"@octavia/bridge": "../dist/bridge.mjs",
		"@octavia/cambiare": "../dist/cambiare.mjs",
		"@octavia/disp": "../dist/disp.mjs",
		"@octavia/state": "../dist/state.mjs",
		"@octavia/xp_basic": "../dist/xp_basic.mjs",
		"@octavia/xp_state": "../dist/xp_state.mjs"
	},
	"publish": {
		"include": [
			"*.json",
			"*.mjs"
		]
	},
	"unstable": [
		"broadcast-channel",
		"ffi",
		"net",
		"webgpu"
	],
	"fmt": {
		"useTabs": true,
		"lineWidth": 200,
		"indentWidth": 4,
		"semiColons": true,
		"exclude": [
			"*"
		]
	},
	"lint": {
		"exclude": [
			"*"
		]
	},
	"nodeModulesDir": false,
	"npmRegistry": "http://127.0.0.1:80/"
}
